{
    "name": "devextreme-data-jaydata",
    "version": "15.1.6",
    "dependencies": {
        "datajs": "latest",
        "jquery": "latest",
        "globalize": "latest",
        "devextreme": "latest",
        "jaydata-itself": "https://raw.githubusercontent.com/jaydata/jaydata/development/release/jaydata.js",
        "jaydata-odata-provider": "https://raw.githubusercontent.com/jaydata/jaydata/development/release/jaydataproviders/oDataProvider.js"
    },
    "devDependencies": {
        "qunit": "latest",
        "sinon": "http://sinonjs.org/releases/sinon-1.16.1.js"
    },
    "install": {
        "path": "lib",

        "sources": {
            "sinon": [
                "bower_components/sinon/index.js"
            ],
            "datajs": [
                "bower_components/datajs/datajs*.min.js"
            ],
            "globalize": [
                "bower_components/globalize/lib/globalize.js"
            ]
        }
    }
}